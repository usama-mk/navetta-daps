<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1e40af;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2563eb;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="truck" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1d4ed8;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2563eb;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="ground" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#4ade80;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#22c55e;stop-opacity:0.2" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bg)"/>
  
  <!-- Ground -->
  <rect x="0" y="450" width="800" height="150" fill="url(#ground)"/>
  
  <!-- Professional team members -->
  <!-- Person 1 -->
  <circle cx="200" cy="350" r="25" fill="#fbbf24"/>
  <rect x="180" y="375" width="40" height="60" fill="#1e40af" rx="5"/>
  <rect x="190" y="385" width="20" height="20" fill="#fbbf24" rx="3"/>
  
  <!-- Person 2 -->
  <circle cx="300" cy="360" r="25" fill="#fbbf24"/>
  <rect x="280" y="385" width="40" height="60" fill="#1e40af" rx="5"/>
  <rect x="290" y="395" width="20" height="20" fill="#fbbf24" rx="3"/>
  
  <!-- Person 3 -->
  <circle cx="400" cy="355" r="25" fill="#fbbf24"/>
  <rect x="380" y="380" width="40" height="60" fill="#1e40af" rx="5"/>
  <rect x="390" y="390" width="20" height="20" fill="#fbbf24" rx="3"/>
  
  <!-- Large professional truck -->
  <rect x="500" y="300" width="250" height="120" fill="url(#truck)" rx="15"/>
  <rect x="520" y="250" width="210" height="70" fill="url(#truck)" rx="35"/>
  
  <!-- Truck wheels -->
  <circle cx="580" cy="440" r="25" fill="#1f2937"/>
  <circle cx="580" cy="440" r="15" fill="#374151"/>
  <circle cx="670" cy="440" r="25" fill="#1f2937"/>
  <circle cx="670" cy="440" r="15" fill="#374151"/>
  
  <!-- Truck details -->
  <rect x="540" y="260" width="30" height="30" fill="#1e40af" rx="5"/>
  <rect x="590" y="260" width="30" height="30" fill="#1e40af" rx="5"/>
  <rect x="640" y="260" width="30" height="30" fill="#1e40af" rx="5"/>
  
  <!-- DAPS logo on truck -->
  <rect x="600" y="320" width="80" height="30" fill="white" rx="5"/>
  <text x="640" y="340" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#2563eb" text-anchor="middle">DAPS</text>
  
  <!-- Junk items being loaded -->
  <rect x="150" y="400" width="40" height="30" fill="#8b5cf6" rx="5"/>
  <rect x="200" y="390" width="35" height="40" fill="#f59e0b" rx="5"/>
  <rect x="250" y="410" width="30" height="25" fill="#ef4444" rx="5"/>
  <rect x="300" y="395" width="45" height="35" fill="#06b6d4" rx="5"/>
  
  <!-- Large dumpster -->
  <rect x="100" y="350" width="120" height="100" fill="#374151" rx="10"/>
  <rect x="110" y="360" width="100" height="80" fill="#6b7280" rx="5"/>
  <rect x="120" y="370" width="80" height="60" fill="#9ca3af" rx="3"/>
  
  <!-- Clouds -->
  <ellipse cx="150" cy="100" rx="60" ry="30" fill="white" opacity="0.7"/>
  <ellipse cx="200" cy="80" rx="40" ry="20" fill="white" opacity="0.7"/>
  <ellipse cx="600" cy="120" rx="50" ry="25" fill="white" opacity="0.7"/>
  <ellipse cx="700" cy="100" rx="40" ry="20" fill="white" opacity="0.7"/>
  
  <!-- Sun -->
  <circle cx="650" cy="80" r="40" fill="#fbbf24" opacity="0.8"/>
  
  <!-- Text overlay -->
  <text x="400" y="150" font-family="Arial, sans-serif" font-size="36" font-weight="bold" fill="white" text-anchor="middle">Professional DAPS Team</text>
  <text x="400" y="180" font-family="Arial, sans-serif" font-size="18" fill="white" text-anchor="middle" opacity="0.9">Loading Furniture & Appliances</text>
  <text x="400" y="205" font-family="Arial, sans-serif" font-size="16" fill="white" text-anchor="middle" opacity="0.8">South Holland, Illinois</text>
  
  <!-- Trust badges -->
  <rect x="50" y="50" width="120" height="40" fill="rgba(255,255,255,0.9)" rx="20"/>
  <text x="110" y="75" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#2563eb" text-anchor="middle">Licensed & Insured</text>
  
  <rect x="630" y="50" width="120" height="40" fill="rgba(255,255,255,0.9)" rx="20"/>
  <text x="690" y="75" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#2563eb" text-anchor="middle">Same-Day Service</text>
</svg>