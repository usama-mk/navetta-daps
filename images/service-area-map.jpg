<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f0f9ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#e0f2fe;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="area" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2563eb;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#3b82f6;stop-opacity:0.1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bg)"/>
  
  <!-- Map outline -->
  <rect x="50" y="50" width="700" height="500" fill="white" stroke="#e2e8f0" stroke-width="2" rx="10"/>
  
  <!-- Service area coverage -->
  <ellipse cx="400" cy="300" rx="250" ry="180" fill="url(#area)" stroke="#2563eb" stroke-width="3" stroke-dasharray="5,5"/>
  
  <!-- Location markers -->
  <!-- South Holland -->
  <circle cx="350" cy="280" r="8" fill="#2563eb"/>
  <text x="350" y="300" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#1e293b" text-anchor="middle">South Holland</text>
  
  <!-- Oak Lawn -->
  <circle cx="420" cy="250" r="8" fill="#2563eb"/>
  <text x="420" y="270" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#1e293b" text-anchor="middle">Oak Lawn</text>
  
  <!-- Tinley Park -->
  <circle cx="480" cy="320" r="8" fill="#2563eb"/>
  <text x="480" y="340" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#1e293b" text-anchor="middle">Tinley Park</text>
  
  <!-- Dolton -->
  <circle cx="320" cy="350" r="6" fill="#3b82f6"/>
  <text x="320" y="365" font-family="Arial, sans-serif" font-size="10" fill="#475569" text-anchor="middle">Dolton</text>
  
  <!-- Harvey -->
  <circle cx="280" cy="300" r="6" fill="#3b82f6"/>
  <text x="280" y="315" font-family="Arial, sans-serif" font-size="10" fill="#475569" text-anchor="middle">Harvey</text>
  
  <!-- Calumet City -->
  <circle cx="380" cy="380" r="6" fill="#3b82f6"/>
  <text x="380" y="395" font-family="Arial, sans-serif" font-size="10" fill="#475569" text-anchor="middle">Calumet City</text>
  
  <!-- Homewood -->
  <circle cx="450" cy="280" r="6" fill="#3b82f6"/>
  <text x="450" y="295" font-family="Arial, sans-serif" font-size="10" fill="#475569" text-anchor="middle">Homewood</text>
  
  <!-- Markham -->
  <circle cx="400" cy="400" r="6" fill="#3b82f6"/>
  <text x="400" y="415" font-family="Arial, sans-serif" font-size="10" fill="#475569" text-anchor="middle">Markham</text>
  
  <!-- Truck icons -->
  <rect x="200" y="200" width="30" height="15" fill="#2563eb" rx="3"/>
  <circle cx="210" cy="220" r="4" fill="#1f2937"/>
  <circle cx="220" cy="220" r="4" fill="#1f2937"/>
  
  <rect x="550" y="400" width="30" height="15" fill="#2563eb" rx="3"/>
  <circle cx="560" cy="420" r="4" fill="#1f2937"/>
  <circle cx="570" cy="420" r="4" fill="#1f2937"/>
  
  <!-- Title -->
  <text x="400" y="100" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#1e293b" text-anchor="middle">DAPS Service Coverage Area</text>
  <text x="400" y="125" font-family="Arial, sans-serif" font-size="14" fill="#64748b" text-anchor="middle">Cook County & South Suburban Chicago</text>
  
  <!-- Legend -->
  <rect x="50" y="500" width="200" height="80" fill="white" stroke="#e2e8f0" rx="5"/>
  <text x="60" y="520" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#1e293b">Service Areas</text>
  
  <circle cx="70" cy="540" r="6" fill="#2563eb"/>
  <text x="85" y="545" font-family="Arial, sans-serif" font-size="10" fill="#475569">Primary Service Areas</text>
  
  <circle cx="70" cy="560" r="4" fill="#3b82f6"/>
  <text x="85" y="565" font-family="Arial, sans-serif" font-size="10" fill="#475569">Extended Coverage</text>
  
  <!-- Stats -->
  <rect x="550" y="500" width="200" height="80" fill="white" stroke="#e2e8f0" rx="5"/>
  <text x="560" y="520" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#1e293b">Coverage Stats</text>
  <text x="560" y="540" font-family="Arial, sans-serif" font-size="10" fill="#475569">8+ Cities Served</text>
  <text x="560" y="555" font-family="Arial, sans-serif" font-size="10" fill="#475569">Same-Day Service</text>
  <text x="560" y="570" font-family="Arial, sans-serif" font-size="10" fill="#475569">Licensed & Insured</text>
</svg>
